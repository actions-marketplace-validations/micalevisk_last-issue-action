{
  "name": "last-issue-action",
  "private": true,
  "version": "1.0.3",
  "description": "GitHub Action to find and export the number of last updated issue that has given labels.",
  "type": "commonjs",
  "author": "Micael Levi L. C. (https://micalevisk.github.io)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/micalevisk/last-issue-action.git"
  },
  "keywords": [
    "github-action",
    "issue",
    "last issue"
  ],
  "scripts": {
    "prepack": "npm run build",
    "prebuild": "npm run clean && npm run compile",
    "build": "ncc build dist/index.js -o bundle",
    "compile": "tsc -p .",
    "watch/compile": "tsc --watch -p .",
    "clean": "rm -rf dist ; rm -rf bundle",
    "exec": "NODE_ENV=development node -r dotenv/config dist"
  },
  "dependencies": {
    "@actions/core": "^1.6.0",
    "@octokit/rest": "^18.12.0"
  },
  "devDependencies": {
    "@types/node": "^16.11.11",
    "@vercel/ncc": "^0.33.0",
    "dotenv": "^10.0.0",
    "typescript": "^4.5.2"
  }
}
